// Functions: Definition, Calls, Recursion

fun greet(name) {
    print "Greetings, " + name + "!";
}

greet("Developer");

fun square(x) {
    return x * x;
}

print "Square of 5 is:";
print square(5);

// Recursive Fibonacci
fun fib(n) {
    if (n < 2) return n;
    return fib(n - 1) + fib(n - 2);
}

var start = clock();
print "Fibonacci(10):";
print fib(10);
var end = clock();
print "Time taken (s):";
print end - start;
